{
  "expo": {
    "name": "NewsApp",
    "slug": "NewsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SystemImg/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SystemImg/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
        ["expo-notifications",{
          "icon": "./assets/SystemImg/icon.png"
        }]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ],
      "googleServicesFile": "./google-services.json",
      "splash": {
        "image": "./assets/SystemImg/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/SystemImg/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.NewsApp.NewsApp"
    },
    "web": {
      "favicon": "./assets/SystemImg/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6bcbccf3-c2a9-4f51-8a82-e41d83e5195f"
      }
    }
  }
}
